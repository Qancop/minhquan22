Code trái tym

#include <stdio.h>

int main() {
 
  printf("\033[31m"); // Chuy?n màu sang d?
    printf("    ****  ****    \n");
    printf("   **        **  \n");
    printf("     **    **    \n");
    printf("        **        \n");
    printf("          \n");
    printf("        I   L O V E   Y O U\n");


    printf("\033[0m"); // Ð?t l?i màu s?c v? m?c d?nh

    return 0;
}
